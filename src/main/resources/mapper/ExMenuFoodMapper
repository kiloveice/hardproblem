<mapper namespace="live.hardproblem.dao.ExMenuFoodMapper">
    <select id="selectFoodIdByMenuId" resultType="java.lang.Integer">
    select food_id
    from menu_food
    where
        menu_id=#{id}
  </select>
</mapper>
